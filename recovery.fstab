/boot            emmc              /dev/block/platform/7824900.sdhci/by-name/boot
/system          ext4              /dev/block/platform/7824900.sdhci/by-name/system
/system_image    emmc              /dev/block/platform/7824900.sdhci/by-name/system
/data	         ext4              /dev/block/platform/7824900.sdhci/by-name/userdata   flags=encryptable=/dev/block/bootdevice/by-name/encrypt
/cache	         ext4              /dev/block/platform/7824900.sdhci/by-name/cache
#/misc	          emmc              /dev/block/platform/7824900.sdhci/by-name/misc
/recovery        emmc              /dev/block/platform/7824900.sdhci/by-name/recovery   flags=backup=1
/efs1            emmc              /dev/block/platform/7824900.sdhci/by-name/modemst1   flags=backup=1;display=EFS
/efs2            emmc              /dev/block/platform/7824900.sdhci/by-name/modemst2   flags=backup=1;subpartitionof=/efs1


/external_sd     vfat             /dev/block/mmcblk1p1  /dev/block/mmcblk1
flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg         vfat              /dev/block/sda1      /dev/block/sda              flags=display="USB-OTG";storage;wipeingui;removable
/sd-ext          ext4             /dev/block/mmcblk1p2    /dev/block/mmcblk2
flags=display="SD-Ext";storage;wipeingui;removable
